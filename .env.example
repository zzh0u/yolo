# ===========================================
# YOLO 项目环境变量配置模板
# 复制此文件为 .env 并填入实际值
# ===========================================

# 数据库配置
POSTGRES_DB=yolo_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=change_this_password
DB_HOST=postgres
DB_PORT=5432
DB_NAME=yolo_db
DB_SSLMODE=disable

# JWT 配置 (请使用强密码)
JWT_SECRET=change_this_jwt_secret_key

# Web3 配置
INJ_EVM_RPC_URL=https://testnet.sentry.tm.injective.network:443
PRIVATE_KEY=your_private_key_here

# 应用配置
GIN_MODE=release
NODE_ENV=production

# 服务端口配置
BACKEND_PORT=8080
FRONTEND_PORT=3000
POSTGRES_PORT=5432

# API 配置
NEXT_PUBLIC_API_URL=http://localhost:8080

# Docker 用户配置
APP_USER_ID=1001
APP_GROUP_ID=1001

# 健康检查配置
HEALTH_CHECK_INTERVAL=30s
HEALTH_CHECK_TIMEOUT=10s
HEALTH_CHECK_RETRIES=3
HEALTH_CHECK_START_PERIOD=40s

# PostgreSQL 健康检查配置
PG_HEALTH_CHECK_INTERVAL=10s
PG_HEALTH_CHECK_TIMEOUT=5s
PG_HEALTH_CHECK_RETRIES=5